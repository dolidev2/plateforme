{% extends 'base.html.twig' %}

{% block title %} {{ service.nomService }} | Ajouter {{ type }}{% endblock %}


{% block content %}
  {% include "commercial/menu.html.twig" %}
  <div class="row">
    <div class="col-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">{{ dos }} {{ type  }} {{ support}}</h4>
                {{ form_start(form) }}
                {{ form_widget(form) }}
                <button type="submit" class="btn btn-primary mr-2">{{ dos }}</button>
                <button type="reset"  class="btn btn-light">Annuler</button>
                {{ form_end(form) }}

            </div>
        </div>
    </div>
 </div>
{% endblock %}